{
  "name": "vlite3",
  "private": false,
  "description": "A Vue 3 UI component library built with Tailwind CSS.",
  "license": "MIT",
  "version": "0.2.17",
  "type": "module",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "author": "safdar-azeem",
  "repository": "https://github.com/safdar-azeem/vlite3",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:vercel": "vue-tsc -b && vite build --config vite.config.vercel.ts",
    "postbuild": "node scripts/bundle-css.js",
    "preview": "vite preview",
    "prepare:dist": "npm run build && cp package.json README.md dist/",
    "release:patch": "npm version patch && npm run prepare:dist && cd dist && npm publish",
    "release:minor": "npm version minor && npm run prepare:dist && cd dist && npm publish",
    "release:major": "npm version major && npm run prepare:dist && cd dist && npm publish",
    "publish:dist": "npm run prepare:dist && cd dist && npm publish"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.js"
    },
    "./style.css": "./style.css"
  },
  "dependencies": {
    "@iconify/vue": "5.0.0",
    "@jaames/iro": "^5.5.2",
    "@tailwindcss/vite": "^4.1.18",
    "@types/scrollreveal": "^0.0.11",
    "@vueuse/core": "^14.1.0",
    "iconify-icon-picker": "^1.0.3",
    "scrollreveal": "^4.0.9",
    "tailwindcss": "^4.1.18",
    "v-datepicker-lite": "^0.1.6",
    "v-tooltip-lite": "^0.2.1",
    "vue-carousel-lite": "^0.2.7",
    "vue-draggable-plus": "^0.5.3",
    "vue3-google-signin": "^2.1.1"
  },
  "peerDependencies": {
    "vue": "^3.5.24",
    "vue-router": "4"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4",
    "vue": "^3.5.24",
    "vue-router": "^4.0.0",
    "vue-tsc": "^3.1.4"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "endOfLine": "auto",
    "bracketSameLine": true
  }
}
